<h1>Dashboard</h1>
<h3>Welcome <%= current_user.username %></h3>
<div>
  <% @posts.each do |post| %>
  <div>
    <%= image_tag image_for(post), size: '64x40' %><%= link_to 'Edit', edit_post_path(post) %> | <%= link_to 'Remove', post_path(post), method: :delete, confirm: { data: "Are You Sure?" } %>
  <% end %>
  </div>
</div>